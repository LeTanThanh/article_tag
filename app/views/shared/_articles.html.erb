<table class="table">
  <thead>
    <tr>
      <th>Id</th>
      <th>Ttile</th>
      <th></th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    <% articles.each do |article| %>
      <tr>
        <td><%= article.id %></td>
        <td><%= article.title %></td>
        <td><%= link_to "Read more ...", article %></td>
        <td><%= link_to "Edit", edit_article_path(article) %></td>
      </tr>
    <% end %>
  </tbody>
</table>
